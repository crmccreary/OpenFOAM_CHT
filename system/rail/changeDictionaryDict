FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dictionaryReplacement
{
    T
    {
        internalField   uniform 400;

        boundaryField
        {
            ".*"
            {
                type            zeroGradient;
                value           uniform 400;
            }
            "rail_to_.*"
            {
                type            compressible::turbulentTemperatureCoupledBaffleMixed;
                Tnbr            T;
                kappa           solidThermo;
                kappaName       none;
                value           uniform 400;
            }

            flux
            {
                type            fixedGradient;
                gradient        uniform 17.19;
            }
        }
    }
}

