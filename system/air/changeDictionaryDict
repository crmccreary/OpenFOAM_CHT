FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dictionaryReplacement
{
    U
    {
        // internalField will be mapped from potential case
        //internalField   uniform (0 0.00037 0);

        boundaryField
        {
            ".*"
            {
                type            fixedValue;
                value           uniform (0 0 0);
            }
            outlet
            {
                //
                // inletOutlet is basically a fixedValue boundary condition when a 
                // flux into the domain is present and a zeroGradient when the flux 
                // is pointing outwards. Though boundary conditions are applied to 
                // boundary patches, this decision is done on a face-to-face basis. 
                // This means that two adjoining boundary faces on a boundary patch 
                // might have fluxes with different signs and are hence handled differently.
                //
                type            inletOutlet;
                inletValue      uniform ( 0 0 0 );
                // value will be mapped from potential case
                //value           uniform ( 0 0.00037 0 );
                //type zeroGradient;
            }

            inlet
            {
                //
                // The outletInlet boundary condition does this decision in the same manner, 
                // but if there is an inward flux, it is a zeroGradient and otherwise a 
                // fixedValue boundary condition.
                //
                type            outletInlet;
                outletValue     uniform ( 0 0 0 );
            }

            inlet_side
            {
                //
                // The outletInlet boundary condition does this decision in the same manner, 
                // but if there is an inward flux, it is a zeroGradient and otherwise a 
                // fixedValue boundary condition.
                //
                type            outletInlet;
                outletValue     uniform ( 0 0 0 );
            }

            x_sym
            {
                type            symmetryPlane;
            }
         
            z_sym_1
            {
                type            symmetryPlane;
            }
         
            z_sym_2
            {
                type            symmetryPlane;
            }

        }
    }

    T
    {
        internalField   uniform 300;

        boundaryField
        {
            ".*"
            {
                type            zeroGradient;
            }

            inlet
            {
                type            inletOutlet;
                inletValue      uniform 300;
                value           uniform 300;
            }

            inlet_side
            {
                type            inletOutlet;
                inletValue      uniform 300;
                value           uniform 300;
            }

            outlet
            {
                type            inletOutlet;
                inletValue      uniform 300;
                value           uniform 300;
            }

            "air_to_.*"
            {
                type            compressible::turbulentTemperatureCoupledBaffleMixed;
                neighbourFieldName T;
                kappa           fluidThermo;
                kappaName       none;
                value           uniform 300;
            }

            x_sym
            {
                type            symmetryPlane;
            }

            z_sym_1
            {
                type            symmetryPlane;
            }

            z_sym_2
            {
                type            symmetryPlane;
            }

        }
    }

    epsilon
    {
        internalField   uniform 25e-3;

        boundaryField
        {
            ".*"
            {
                type            compressible::epsilonWallFunction;
                Cmu             0.09;
                kappa           0.41;
                E               9.8;
                value           uniform 25e-3;
            }

            inlet
            {
                type            inletOutlet;
                inletValue      uniform 25e-3;
                value           uniform 25e-3;
            }

            inlet_side
            {
                type            inletOutlet;
                inletValue      uniform 25e-3;
                value           uniform 25e-3;
            }

            outlet
            {
                type            inletOutlet;
                inletValue      uniform 25e-3;
                value           uniform 25e-3;
            }

            x_sym
            {
                type            symmetryPlane;
            }

            z_sym_1
            {
                type            symmetryPlane;
            }

            z_sym_2
            {
                type            symmetryPlane;
            }

        }
    }

    k
