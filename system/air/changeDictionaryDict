FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dictionaryReplacement
{
    U
    {
        internalField   uniform (0 0.1 0);

        boundaryField
        {
            ".*"
            {
                type            fixedValue;
                value           uniform (0 0 0);
            }
            inlet
            {
                type            inletOutlet;
                inletValue      uniform ( 0 0 0 );
                value           uniform ( 0 0.1 0 );
            }
            outlet
            {
                type            inletOutlet;
                inletValue      uniform ( 0 0 0 );
                value           uniform ( 0 0.1 0 );
            }
            x_sym
            {
                type            symmetryPlane;
            }
            z_sym_1
            {
                type            symmetryPlane;
            }
            z_sym_2
            {
                type            symmetryPlane;
            }
        }
    }

    T
    {
        internalField   uniform 300;

        boundaryField
        {
            ".*"
            {
                type            zeroGradient;
            }

            inlet
            {
                type            inletOutlet;
                inletValue      uniform 300;
                value           uniform 300;
            }
            outlet
            {
                type            inletOutlet;
                inletValue      uniform 300;
                value           uniform 300;
            }

            "air_to_.*"
            {
                type            compressible::turbulentTemperatureCoupledBaffleMixed;
                Tnbr            T;
                kappa           fluidThermo;
                kappaName       none;
                value           uniform 300;
            }
