            {
                type            fixedValue;
                value           uniform 1.0e5;
            }

            inlet_side
            {
                type            fixedValue;
                value           uniform 1.0e5;
            }

            outlet
            {
                //type            fixedFluxPressure;
                //value           uniform 1.0e5;
                //type            buoyantPressure;
                //value           uniform 1.0e5;
                type            inletOutlet;
                inletValue      uniform 1.0e5;
            }

            x_sym
            {
                type            symmetryPlane;
