FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0.00037 0);

boundaryField
{
    x_sym
    {
        type            symmetryPlane;
    }
    z_sym_1
    {
        type            symmetryPlane;
    }
    z_sym_2
    {
        type            symmetryPlane;
    }
    inlet
    {
        type            fixedValue;
        value           uniform (0 0.01 0);
    }
    inlet_side
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    wall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform ( 0 0 0 );
    }
}


